<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abhishekanand.android.firstlogin"
    android:versionCode="1"
    android:versionName="1.0.1">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fl17"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.abhishekanand.android.firstlogin.MainActivity"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.Categories_accounts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.Categories_accounts" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.AccDetails"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.MainActivity" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.AddContent"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.MainActivity" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.Categories_accounts"
            android:label="First LogIn" />
        <activity
            android:name="com.abhishekanand.android.firstlogin.ChangePassword"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.Categories_accounts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.Categories_accounts" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.LoginPassword"
            android:label="First LogIn"></activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.SearchResult"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.Categories_accounts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.Categories_accounts" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.AlternateLogin"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.LoginPassword">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.LoginPassword" />
        </activity>
        <activity
            android:name="com.abhishekanand.android.firstlogin.ChangeAlternate"
            android:label="First LogIn"
            android:parentActivityName="com.abhishekanand.android.firstlogin.Categories_accounts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.Categories_accounts" />
        </activity>
        <activity android:name="com.abhishekanand.android.firstlogin.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abhishekanand.android.firstlogin.AboutUs">
            android:label="About Us"
            android:parentActivityName=".Categories_accounts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.abhishekanand.android.firstlogin.Categories_accounts" />
        </activity>
    </application>

</manifest>
